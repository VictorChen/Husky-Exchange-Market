<div class="products">
	<span class="user"><%= link_to feed_item.user.name, feed_item.user %> is selling <%= feed_item.name %></span>
	<div class="description"><%= feed_item.description %></div>
	<div class="info">
		<div class="price">
			$<%= feed_item.price %>
		</div>
		<div class="timestamp">
		Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
		</div>
	</div>
</div>